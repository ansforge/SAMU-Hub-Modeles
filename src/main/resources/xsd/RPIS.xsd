                <annotation>
                    <documentation>s'exprime au format ISO 8601 YYY-MM-DDThh:mm:ss</documentation>
                </annotation>
            </element>
            <element name="returnSmur" type="dateTime">
                <annotation>
                    <documentation>s'exprime au format ISO 8601 YYY-MM-DDThh:mm:ss</documentation>
                </annotation>
            </element>
        </sequence>
    </complexType>
    <complexType name="detailedAddress">
        <sequence>
            <element name="inseeCode">
                <annotation>
                    <documentation>Code INSEE de la commune actuelle sur la base du Code Officiel géographique en vigueur. Obligatoire si le nom de la commune est renseigné.
Le Code INSEE peut également précisé le pays d'intervention, si étranger. </documentation>
                </annotation>
                <simpleType>
                    <restriction base="string">
                        <pattern value="[0-9]{5}"/>
                    </restriction>
                </simpleType>
            </element>
            <element name="city" type="string">
                <annotation>
                    <documentation>Nom officiel de la commune actuelle</documentation>
                </annotation>
            </element>
        </sequence>
    </complexType>
