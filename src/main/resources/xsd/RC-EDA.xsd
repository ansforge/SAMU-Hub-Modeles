            <element name="creation" type="dateTime">
                <annotation>
                    <documentation>Groupe date heure de création de la décision.  L'indicateur de fuseau horaire Z ne doit pas être utilisé.</documentation>
                </annotation>
            </element>
            <element name="type">
                <annotation>
                    <documentation>Type de décision prise</documentation>
                </annotation>
                <simpleType>
                    <restriction base="string">
                        <enumeration value="CONSEIL"/>
                        <enumeration value="PMT"/>
                        <enumeration value="INTER"/>
                        <enumeration value="ORIENT"/>
                        <enumeration value="PASPLUS"/>
                    </restriction>
                </simpleType>
            </element>
            <element minOccurs="0" name="transportDetails" type="x:transportDetails"/>
        </sequence>
    </complexType>
