<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns:x="https://hub.esante.gouv.fr/EDXL-DE-envelope-only.xsd" elementFormDefault="qualified"
        targetNamespace="https://hub.esante.gouv.fr/EDXL-DE-envelope-only.xsd" xmlns="http://www.w3.org/2001/XMLSchema">
    <complexType name="EDXL-DE-envelope-only">
        <sequence>
            <element minOccurs="0" name="edxlMessage" type="x:EdxlMessage"/>
            <element minOccurs="0" name="edxlEnvelope" type="x:EdxlEnvelope"/>
            <element name="content" type="x:Content"/>
            <element minOccurs="0" name="contentObject" type="x:ContentObject"/>
            <element minOccurs="0" name="contentXML" type="x:ContentXML"/>
            <element minOccurs="0" name="embeddedXMLContent" type="x:EmbeddedXMLContent"/>
            <element name="descriptor" type="x:Descriptor"/>
            <element name="distributionKind" type="x:DistributionKind"/>
            <element name="distributionStatus" type="x:DistributionStatus"/>
            <element minOccurs="0" name="explicitAddress" type="x:ExplicitAddress"/>
        </sequence>
    </complexType>
    <complexType name="EdxlEnvelope">
        <sequence>
            <element name="senderID" type="string"/>
            <element name="distributionID" type="string"/>
            <element name="dateTimeSent" type="dateTime"/>
            <element name="dateTimeExpires" type="dateTime"/>
            <element name="distributionStatus" type="x:DistributionStatus"/>
            <element name="distributionKind" type="x:DistributionKind"/>
            <element name="descriptor" type="x:Descriptor"/>
        </sequence>
    </complexType>
    <complexType name="EdxlMessage">
        <sequence>
            <element name="content" type="x:Content"/>
        </sequence>
    </complexType>
    <complexType name="Content">
        <sequence>
            <element maxOccurs="unbounded" minOccurs="1" name="ContentObject" type="x:ContentObject"/>
        </sequence>
    </complexType>
    <complexType name="ContentObject">
        <sequence>
            <element name="contentXML" type="x:ContentXML"/>
        </sequence>
    </complexType>
    <complexType name="ContentXML">
        <sequence>
            <element name="embeddedXMLContent" type="x:EmbeddedXMLContent"/>
        </sequence>
    </complexType>
    <complexType name="EmbeddedXMLContent">
        <sequence>
            <element name="embeddedJsonContent" type="x:AnyXMLType"/>
        </sequence>
    </complexType>
    <complexType name="AnyXMLType">
    </complexType>
    <complexType name="Descriptor">
        <sequence>
            <element name="explicitAddress" type="x:ExplicitAddress"/>
            <element name="language" type="string"/>
        </sequence>
    </complexType>
    <simpleType name="DistributionKind">
        <restriction base="string">
            <enumeration value="Report"/>
            <enumeration value="Update"/>
            <enumeration value="Cancel"/>
            <enumeration value="Ack"/>
            <enumeration value="Error"/>
        </restriction>
    </simpleType>
    <simpleType name="DistributionStatus">
        <restriction base="string">
            <enumeration value="Actual"/>
            <enumeration value="Exercise"/>
        </restriction>
    </simpleType>
    <complexType name="ExplicitAddress">
        <sequence>
            <element name="explicitAddressScheme" type="string"/>
            <element name="explicitAddressValue" type="string"/>
        </sequence>
    </complexType>
</schema>